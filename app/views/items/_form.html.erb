<%=  form_for @item, :html => {:multipart => true} do |f| %>
 
  <div class="field">
     <%= f.label :name %>
     <%= f.text_field :name %>
  </div>

  <div class="field">
     <%= f.label :description %>
     <%= f.text_field :description %>
  </div>
  <div class="field">
    <%= f.label :photo %>
    <%= f.file_field :photo %>
  </div>

 
   

   <div class="add_item"><%= f.submit %></div>
<% end %>